@article{lin:Cell:2012,
	abstract = {Elevated expression of the c-Myc transcription factor occurs frequently
	in human cancers and is associated with tumor aggression and poor
	clinical outcome. The effect of high levels of c-Myc on global gene
	regulation is poorly understood but is widely thought to involve
	newly activated or repressed "Myc target genes." We report here that
	in tumor cells expressing high levels of c-Myc the transcription
	factor accumulates in the promoter regions of active genes and causes
	transcriptional amplification, producing increased levels of transcripts
	within the cell's gene expression program. Thus, rather than binding
	and regulating a new set of genes, c-Myc amplifies the output of
	the existing gene expression program. These results provide an explanation
	for the diverse effects of oncogenic c-Myc on gene expression in
	different tumor cells and suggest that transcriptional amplification
	reduces rate-limiting constraints for tumor cell growth and proliferation.},
	author = {Lin, Charles Y and Lov{\'e}n, Jakob and Rahl, Peter B and Paranal, Ronald M and Burge, Christopher B and Bradner, James E and Lee, Tong Ihn and Young, Richard A},
	date-added = {2014-03-17 15:30:02 +0000},
	date-modified = {2014-03-17 15:30:12 +0000},
	doi = {10.1016/j.cell.2012.08.026},
	journal = {Cell},
	journal-full = {Cell},
	mesh = {Cell Line, Tumor; Cell Proliferation; Enhancer Elements, Genetic; Gene Expression Regulation, Neoplastic; Humans; Neoplasms; Promoter Regions, Genetic; Proto-Oncogene Proteins c-myc; Transcription, Genetic},
	pages = {56-67},
	pmc = {PMC3462372},
	pmid = {23021215},
	pst = {ppublish},
	title = {Transcriptional amplification in tumor cells with elevated c-Myc},
	volume = {151},
